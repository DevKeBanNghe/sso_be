    "husky": "^9.0.11",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "18.0.5",
    "prettier": "^2.6.2",
    "prisma": "^5.12.1",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.3.2",
    "webpack-cli": "^5.1.4"
  },
  "nx": {
    "includedScripts": []
  }
}
