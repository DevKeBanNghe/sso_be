{
  "name": "@nestjs-microservice/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "test:watch": "nx run test -- --watch",
    "e2e": "nx e2e e2e",
    "e2e:watch": "npm run test:e2e -- --watch",
    "format": "nx format:write",
    "lint": "nx lint --fix",
    "pris-init": "(if exist prisma rmdir /s /q prisma) && cross-env-shell prisma init --datasource-provider $DBNAME --generator-provider prisma-client-$DBNAME --output ../node_modules/@prisma/${DBNAME}_client && mv prisma/* prisma/schema.%DBNAME%.prisma",
    "pris-init-pg": "cross-env DBNAME=postgresql npm run pris-init",
    "pris-init-multiple": "bash ./scripts/multiple_prisma",
    "pris-init-mongo": "cross-env DBNAME=mongodb npm run pris-init",
    "pris-mig": "cross-env-shell prisma migrate dev --name=$DBNAME --schema=./prisma/schema.$DBNAME.prisma",
    "pris-mig-pg": "cross-env DBNAME=postgresql npm run pris-mig && npm run pris-gen-pg",
    "pris-push": "cross-env-shell prisma db push --schema=./prisma/schema.$DBNAME.prisma",
    "pris-push-mongo": "cross-env DBNAME=mongodb npm run pris-push",
    "pris-gen": "cross-env-shell prisma generate --schema=./prisma/schema.$DBNAME.prisma",
    "pris-gen-pg": "cross-env DBNAME=postgresql npm run pris-gen",
    "init-husky": "husky && type husky-prepare\\pre-commit > .husky/pre-commit && type husky-prepare\\commit-msg > .husky/commit-msg",
    "postinstall": "(if exist .husky rmdir /s /q .husky) && npm run init-husky && npm run pris-gen-pg"
  },
  "private": true,
  "dependencies": {
    "@nestjs-microservice/source": "file:",
    "@nestjs-modules/mailer": "^1.11.2",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^10.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/throttler": "^5.1.2",
    "@prisma/client": "^5.12.1",
    "@types/express-useragent": "^1.0.5",
    "@types/ms": "^0.7.34",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express-useragent": "^1.0.15",
    "helmet": "^7.1.0",
    "joi": "^17.12.2",
    "mongoose": "^8.2.1",
    "mongoose-autopopulate": "^1.1.0",
    "mongoose-timestamp": "^0.6.0",
    "ms": "^2.1.3",
    "nest-winston": "^1.9.4",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "pug": "^3.0.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "18.0.5",
    "@nx/eslint-plugin": "18.0.5",
    "@nx/jest": "18.0.5",
    "@nx/js": "18.0.5",
    "@nx/nest": "18.0.5",
    "@nx/node": "18.0.5",
    "@nx/web": "18.0.5",
    "@nx/webpack": "18.0.5",
    "@nx/workspace": "18.0.5",
    "@swc-node/register": "~1.8.0",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.16.9",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "cross-env": "^7.0.3",
    "eslint": "~8.48.0",
    "eslint-config-prettier": "^9.0.0",
